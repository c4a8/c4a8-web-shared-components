{
  "name": "shared-components",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "storybook": "node scripts/fix-storybook.js && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "module:after-build": "node scripts/prepare-styles.js",
    "module:build": "node scripts/copy-components.js && nuxt-module-build build && npm run module:after-build",
    "temp": "npm run module:build && node scripts/move-module.js",
    "new-version": "cd .. && npm version minor --no-git-tag-version && cd nuxt && npm run temp",
    "patch": "cd .. && npm version patch --no-git-tag-version && cd nuxt && npm run temp"
  },
  "dependencies": {
    "@nuxt/content": "^3.4.0",
    "@nuxt/kit": "^3.16.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxtjs/i18n": "^9.3.4",
    "@nuxtjs/sitemap": "^7.2.5",
    "@types/jquery": "^3.5.32",
    "@pinia/nuxt": "^0.10.1",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^4.5.3",
    "cssnano": "^7.0.6",
    "ion-rangeslider": "^2.3.1",
    "jquery": "^3.7.1",
    "node-html-parser": "^6.1.13",
    "nuxt": "^3.16.1",
    "nuxt-seo-utils": "^7.0.4",
    "pinia": "^3.0.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-url": "^10.1.3",
    "sass": "^1.80.2",
    "sitemap": "^8.0.0",
    "slick-carousel": "^1.8.1",
    "vue": "3.5.11",
    "vue-router": "4.4.5",
    "vue3-lottie": "^3.3.1"
  },
  "devDependencies": {
    "@nuxtjs/storybook": "^8.3.3",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@storybook/test": "8.4.7",
    "gray-matter": "^4.0.3",
    "storybook": "8.4.7"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
